{
    "version": 2,
    "builds": [
      {
        "src": "django-crud-app/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "django-crud-app/wsgi.py"
      }
    ]
  }